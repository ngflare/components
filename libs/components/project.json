{
  "name": "components",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "libs/components/src",
  "prefix": "f",
  "tags": [],
  "projectType": "library",
  "targets": {
    "build": {
      "executor": "@nx/angular:package",
      "outputs": ["{workspaceRoot}/dist"],
      "options": {
        "project": "libs/components/ng-package.json"
      },
      "configurations": {
        "production": {
          "tsConfig": "libs/components/tsconfig.lib.prod.json"
        },
        "development": {
          "tsConfig": "libs/components/tsconfig.lib.json"
        }
      },
      "defaultConfiguration": "production"
    },
    "test": {
      "executor": "@nx/jest:jest",
      "outputs": ["{workspaceRoot}/coverage/{projectRoot}"],
      "options": {
        "jestConfig": "libs/components/jest.config.ts"
      }
    },
    "lint": {
      "executor": "@nx/eslint:lint",
      "outputs": ["{options.outputFile}"]
    },
    "storybook": {
      "executor": "@storybook/angular:start-storybook",
      "options": {
        "port": 4400,
        "configDir": "libs/components/.storybook",
        "browserTarget": "components:build-storybook",
        "compodoc": false,
        "styles": ["libs/components/src/lib/components/button/button.css"]
      },
      "configurations": {
        "ci": {
          "quiet": true
        }
      }
    },
    "build-storybook": {
      "executor": "@storybook/angular:build-storybook",
      "outputs": ["{options.outputDir}"],
      "options": {
        "outputDir": "dist/storybook/components",
        "configDir": "libs/components/.storybook",
        "browserTarget": "components:build-storybook",
        "compodoc": false
      },
      "configurations": {
        "ci": {
          "quiet": true
        }
      }
    },
    "test-storybook": {
      "executor": "nx:run-commands",
      "options": {
        "command": "test-storybook -c libs/components/.storybook --url=http://localhost:4400"
      }
    },
    "build-assets": {
      "executor": "nx:run-script",
      "options": {
        "script": "build-assets"
      }
    }
  }
}
